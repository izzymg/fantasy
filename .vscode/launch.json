  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Production server",
      "program": "${workspaceFolder}/server/api/api.js",
      "env": {
        "NODE_ENV": "production",
        "LOG_LEVEL": "fatal"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug server",
      "program": "${workspaceFolder}/server/api/api.js",
      "env": {
        "NODE_ENV": "development",
        "LOG_LEVEL": "info"
      }
    }
  ]
}